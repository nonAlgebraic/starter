{
	"compilerOptions": {
		"moduleResolution": "Node",
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"importsNotUsedAsValues": "error",
		"noEmit": true,
		"jsx": "react"
	}
}
